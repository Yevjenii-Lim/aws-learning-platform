# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# DynamoDB Tables
AWS_DYNAMODB_SERVICES_TABLE=aws-learning-services
AWS_DYNAMODB_TUTORIALS_TABLE=aws-learning-tutorials
AWS_DYNAMODB_PROGRESS_TABLE=aws-learning-user-progress

# S3 Bucket
AWS_S3_BUCKET=aws-learning-platform-content

# Optional: AWS Lambda API Gateway (if using serverless)
AWS_API_GATEWAY_URL=https://your-api-gateway-url.amazonaws.com

# Next.js Configuration
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# AWS Cognito Configuration
COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
COGNITO_CLIENT_SECRET=your_client_secret_here

# Optional: Debug Mode
DEBUG_COGNITO=false 